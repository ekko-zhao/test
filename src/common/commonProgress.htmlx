<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
</head>
<body>
	<div id="div_progress_dialog" width="100%" height="100%"
		visibility="false" position="relative" left="0" top="0"
		backgroundColor="#000000" opacity="0.5"></div>
	<div id="div_progress_dialog2" width="100%" height="100%"
		visibility="false" position="relative" vAlign="middle" hAlign="center">
		<progress id="commonProgressDialog" width="50" height="50" />
	</div>
</body>
<script>
	function showProgressDialog() {
		div_progress_dialog.setVisibility(true);
		div_progress_dialog2.setVisibility(true);
	}
	function dismissProgressDialog() {
		div_progress_dialog.setVisibility(false);
		div_progress_dialog2.setVisibility(false);
	}
</script>
</html>
